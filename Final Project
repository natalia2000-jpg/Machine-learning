        "lin_reg = LinearRegression()\n",
        "lin_reg.fit(X_train, y_train)\n",
        "\n",
        "lin_preds = lin_reg.predict(X_val)\n",
        "lin_rmse = rmse(y_val, lin_preds)\n",
        "lin_rmse\n"

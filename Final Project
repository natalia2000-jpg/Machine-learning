      "cell_type": "markdown",
      "source": [
        "#4.Επικύρωση Αλγορίθμων"
      ],
      "metadata": {
        "id": "1_yAYK6t5XIr"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "y = np.log1p(y_consumption)\n",
        "\n",
        "X_train, X_val, y_train, y_val = train_test_split(\n",
        "    x, y, test_size=0.2, random_state=42\n",
        ")\n",
        "\n",
        "def rmse(y_true, y_pred):\n",
        "    return np.sqrt(np.mean((y_true - y_pred)**2))\n",
        "\n",
        "\n",
        "param_grid = [\n",
        "    {'n_estimators': 300, 'max_depth': 15, 'min_samples_leaf': 5, 'max_features': 0.2},\n",
        "    {'n_estimators': 400, 'max_depth': 20, 'min_samples_leaf': 5, 'max_features': 0.3 },\n",
        "    {'n_estimators': 500, 'max_depth': 25, 'min_samples_leaf': 3, 'max_features': 0.3 }, # Changed 'auto' to 'sqrt'\n",
        "    {'n_estimators': 300, 'max_depth': 20, 'min_samples_leaf': 10, 'max_features': 0.3},\n",
        "]\n",
        "\n",
        "best_rmse = np.inf\n",
        "best_params = None\n",
        "best_model = None\n",
        "\n",
        "for params in param_grid:\n",
        "    rf = RandomForestRegressor(\n",
        "        n_estimators=params['n_estimators'],\n",
        "        max_depth=params['max_depth'],\n",
        "        min_samples_leaf=params['min_samples_leaf'],\n",
        "        max_features=params['max_features'],\n",
        "        random_state=42,\n",
        "        n_jobs=-1\n",
        "    )\n",
        "    rf.fit(X_train, y_train)\n",
        "    preds = rf.predict(X_val)\n",
        "    score = rmse(y_val, preds)\n",
        "    print(f\"Params: {params}, RMSE: {score:.4f}\")\n",
        "\n",
        "    if score < best_rmse:\n",
        "        best_rmse = score\n",
        "        best_params = params\n",
        "        best_model = rf\n",
        "\n",
        "# --- 5. Αποτελέσματα ---\n",
        "print(\"\\n=== Best Parameters ===\")\n",
        "print(best_params)\n",
        "print(\"Validation RMSE (log-scale):\", best_rmse)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "0MBe301R8mfA",

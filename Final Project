      "cell_type": "code",
      "source": [
        "poverty_rows = []\n",
        "\n",
        "for sid in test_survey_ids.unique():\n",
        "    mask = test_survey_ids == sid\n",
        "    survey_preds = test_preds[mask]\n",
        "\n",
        "    row = {'survey_id': sid}\n",
        "    for t in poverty_thresholds:\n",
        "        row[f'pct_hh_below_{t}'] = (survey_preds < float(t)).mean()\n",
        "\n",
        "    poverty_rows.append(row)\n",
        "\n",
        "predicted_poverty_distribution = pd.DataFrame(poverty_rows)\n",
        "\n",
        "predicted_poverty_distribution.to_csv(\n",
        "    'predicted_poverty_distribution.csv',\n",
